static int[] GeradorCPF()
        {
            int[] cpf = new int[11];
            for (int i = 0; i < cpf.Length - 2; i++)
            {
                Random r = new Random();
                cpf[i] = r.Next(1, 9);
            }
            return cpf;
        }
static bool ValidarCPF(int[] cpf)
        {
            bool valido = false;

            return valido;
        }
